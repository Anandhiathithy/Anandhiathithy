package Homeworksum;
import java.util.*;
public class Element3rdpostionofarraylist {


	public void thirdposition()
	    {
	    ArrayList<Integer> al=new ArrayList<>();
	      {
	        al.add(5);
	        al.add(3);
	        al.add(4);
	        al.add(5);
	        al.add(1);
	        al.add(3);
	        System.out.println("Elements in an Arraylist :"+ al);
	        System.out.println("Elements in the 3rd position  :"+ al.get(2));


	      }
	    }
	public static void main(String args[])
	{
		Element3rdpostionofarraylist a=new Element3rdpostionofarraylist();
		a.thirdposition();
		

	}
	}
